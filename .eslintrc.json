{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "only-warn"
  ],
  "rules": {
    "prettier/prettier": [
      "off"
    ],
    "no-console": "warn",
    "eqeqeq": "warn",
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}